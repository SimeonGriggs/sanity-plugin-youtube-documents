{
  "name": "sanity-plugin-youtube-documents",
  "version": "2.0.1",
  "description": "Import YouTube videos as individual documents",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/SimeonGriggs/sanity-plugin-youtube-documents#readme",
  "bugs": {
    "url": "https://github.com/SimeonGriggs/sanity-plugin-youtube-documents/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SimeonGriggs/sanity-plugin-youtube-documents.git"
  },
  "license": "MIT",
  "author": "Simeon Griggs <simeon@hey.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "clean": "rimraf dist",
    "dev": "pkg-utils watch --strict & (cd studio && pnpm dev)",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "studio:install": "npm install --prefix studio",
    "watch": "pkg-utils watch --strict"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "overrides": {
    "conventional-changelog-conventionalcommits": ">= 8.0.0"
  },
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/studio-secrets": "^3.0.1",
    "@sanity/ui": "^3.0.14",
    "get-youtube-id": "^1.0.1",
    "sanity-plugin-utils": "^1.6.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@sanity/pkg-utils": "^6.12.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "20",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "sanity": "^5.0.0",
    "semantic-release": "^24.2.3",
    "styled-components": "^6.1.19",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "^19",
    "sanity": "^5"
  },
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=20"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "nodeEngine": false,
      "sanityV2Json": false,
      "eslintImports": false
    }
  }
}

{
  "name": "sanity-plugin-youtube-documents",
  "version": "1.0.0",
  "description": "Import YouTube videos as individual documents",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/SimeonGriggs/sanity-plugin-youtube-documents#readme",
  "bugs": {
    "url": "https://github.com/SimeonGriggs/sanity-plugin-youtube-documents/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SimeonGriggs/sanity-plugin-youtube-documents.git"
  },
  "license": "MIT",
  "author": "Simeon Griggs <simeon@hey.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/studio-secrets": "^3.0.1",
    "@sanity/ui": "^2.10.12",
    "get-youtube-id": "^1.0.1",
    "sanity-plugin-utils": "^1.6.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@sanity/pkg-utils": "^6.12.2",
    "@sanity/plugin-kit": "^4.0.19",
    "@sanity/semantic-release-preset": "^5.0.0",
    "@types/react": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanity": "^3.68.3",
    "styled-components": "^6.1.13",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  }
}
